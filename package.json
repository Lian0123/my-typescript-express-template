{
  "name": "my-typescript-express-template",
  "version": "0.0.1",
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "scripts": {
    "preinstall": "chmod +x ./script/shell/*",
    "prebuild": "rimraf dist",
    "build": "tsc",
    "postbuild": "sh ./script/shell/update-build-file.sh",
    "lint": "eslint . --ext .ts  --fix ",
    "start:dev": "nodemon ./src/main.ts",
    "start:prod": "node ./dist/main.js",
    "test:once": "jest --forceExit",
    "test:e2e": "jest \".e2e.spec.ts$\" --forceExit",
    "test": "jest \"(?<!.e2e).spec.ts$\" --detectOpenHandles --coverage",
    "docker-compose:local": "docker-compose -f docker-compose.local.yaml --env-file ./envs/.env",
    "docker-compose:test": "docker-compose -f docker-compose.test.yaml --env-file ./envs/.env",
    "migration:create": "npx typeorm migration:create --dir=./src/migrations/ -n ",
    "migration:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run --config ./src/typeorm-migration-config.ts",
    "migration:revert": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:revert --config ./src/typeorm-migration-config.ts",
    "prepare": "husky install",
    "env:change": "./script/shell/update-env-file.sh -c",
    "db:diagram": "ts-node --script-mode ./script/typescript/create-db-uml.ts",
    "openapi:generate": "./script/shell/create-openapi-generate.sh "
  },
  "nodemonConfig": {
    "watch": [
      "./src/**/*.ts"
    ],
    "ext": "ts, html",
    "ignore": [
      "./src/public"
    ]
  },
  "eslintIgnore": [
    "src/public/"
  ],
  "dependencies": {
    "amqplib": "0.8.0",
    "body-parser": "1.20.1",
    "bull": "4.2.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "command-line-args": "5.2.0",
    "cookie-parser": "1.4.6",
    "dayjs": "1.11.3",
    "dotenv": "10.0.0",
    "express": "4.21.2",
    "express-async-errors": "3.1.1",
    "express-jwt": "8.2.0",
    "express-rate-limit": "6.0.1",
    "helmet": "5.0.1",
    "http-status-codes": "2.2.0",
    "i18next": "21.6.3",
    "i18next-express-middleware": "2.0.0",
    "i18next-node-fs-backend": "2.1.3",
    "inversify": "6.0.1",
    "inversify-express-utils": "6.4.6",
    "inversify-socket-utils": "1.0.1136",
    "ioredis": "4.28.2",
    "jest": "27.1.0",
    "jest-express": "1.12.0",
    "jet-logger": "1.0.6",
    "jsonfile": "6.1.0",
    "mathjs": "10.0.1",
    "module-alias": "2.2.2",
    "mongodb": "^3.7.4",
    "morgan": "1.10.0",
    "node-mocks-http": "1.11.0",
    "pg": "8.7.1",
    "pino": "7.6.2",
    "pino-http": "8.2.0",
    "rate-limit-redis": "2.1.0",
    "reflect-metadata": "0.1.13",
    "semantic-release": "21.0.1",
    "socket.io": "^4.8.0",
    "socket.io-client": "^4.8.0",
    "swagger-express-ts": "1.1.0",
    "swagger-ui-express": "4.3.0",
    "ts-jest": "27.1.2",
    "typeorm": "0.2.42-dev.cefddd9",
    "typeorm-i18n": "0.2.0-rc.1",
    "typeorm-transactional-cls-hooked": "^0.1.21",
    "typescript": "4.5.4",
    "uuid": "8.3.2",
    "valid-env": "1.1.1"
  },
  "devDependencies": {
    "@jest-mock/express": "1.4.5",
    "@types/amqplib": "^0.8.2",
    "@types/command-line-args": "5.2.0",
    "@types/cookie-parser": "1.4.2",
    "@types/express": "4.17.13",
    "@types/find": "0.2.1",
    "@types/fs-extra": "9.0.13",
    "@types/jasmine": "3.10.2",
    "@types/jest": "27.0.3",
    "@types/jsonfile": "6.0.1",
    "@types/morgan": "1.9.3",
    "@types/node": "^17.0.7",
    "@types/supertest": "2.0.11",
    "@types/swagger-ui-express": "4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "@webpack-cli/generators": "^3.0.1",
    "commitizen": "4.2.5",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^7.32.0",
    "eslint-config-google": "0.14.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "find": "0.3.0",
    "fs-extra": "10.0.0",
    "husky": "7.0.0",
    "jasmine": "3.10.0",
    "nodemon": "^3.0.1",
    "rimraf": "3.0.2",
    "supertest": "6.1.6",
    "ts-node": "10.4.0",
    "tsconfig-paths": "3.12.0",
    "typeorm-uml": "1.6.4",
    "typescript": "4.5.4",
    "webpack": "5.76.0",
    "webpack-cli": "4.9.1"
  }
}
